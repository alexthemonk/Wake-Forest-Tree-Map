    <h1>Add a Tree</h1>

    <form action="/data/tree/create" method="post">
  		Short Name:<br>
  		<input type="text" name="tree_label" value="">
  		<br>
  		Common Name:<br>
  		<input type="text" name="common_name" value="">
  		<br>
      Longitude:<br>
      <input type="text" name="longitude" value="">
      <br>
      Latitude:<br>
      <input type="text" name="latitude" value="">
      <br>
      Date and Time:<br>
      <input type="text" name="date_collected" value="">
      <br>
      Height:<br>
      <input type="text" name="height" value="">
      <br>
      Diameter at Breast Height:<br>
      <input type="text" name="DBH" value="">
      <br>

      Branch 1 (cm)<br>
      <input type="text" name="Branch 1 (cm)" value="">
      <br>
      Branch 2 (cm):<br>
      <input type="text" name="Branch 2 (cm)" value="">
      <br>
      Branch 3 (cm):<br>
      <input type="text" name="Branch 3 (cm)" value="">
